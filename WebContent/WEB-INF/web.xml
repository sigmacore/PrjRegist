<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  
  <display-name>PrjRegist</display-name>
  
    <servlet>
        <servlet-name>registerCHN</servlet-name>
        <servlet-class>com/reg/web/Controller.java</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>registerCHN</servlet-name>
        <url-pattern>/PrjRegist/jsp/registerCHN.jsp</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com/reg/web/Controller.java</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/PrjRegist/jsp/register.jsp</url-pattern>
    </servlet-mapping>
  
  
  <welcome-file-list>
    <welcome-file>/jsp/default.jsp</welcome-file>
  </welcome-file-list>
</web-app>